        ArrayList<Media> modifiedList = new ArrayList<Media>() {
            {
                add(new VideoGame(Genre.ACTION, "Uncharted 1", 54, 6.70, 2, true));
                add(new VideoGame(Genre.ACTION, "Uncharted 2", 37, 6.70, 2, false));
